{
    "name": "lotfio-lakehal/timino",
    "type": "framework",
    "description": "Timino is a simple, lightweight and S.O.L.I.D PHP MVC framework",
    "keywords": ["timino","tim", "php","php mvc", "php simple framework", "php solid", "php lightweight mvc"],
    "homepage": "https://github.com/lotfio-lakehal/Timino",
    "license": "MIT",
    "authors": [
        {
            "name": "lotfio lakehal",
            "email": "lotfiolakehal@gmail.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.0",
        "lotfio-lakehal/console": "^0.3.2"
    },

    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },

    "autoload":{
        "psr-4": {
            "App\\" : "app/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Tests\\":"tests/"
        }
    },

    "repositories" : [

        {
            "type" : "path",
            "url"  : "/var/www/timino/packages/timino-console/"
        }
    ]
}
